{
    "name": "beyojar-mobile",
    "version": "1.0.0",
    "main": "index.ts",
    "scripts": {
        "start": "expo start",
        "android": "expo start --android",
        "ios": "expo start --ios",
        "web": "expo start --web",
        "test": "jest --watchAll",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "storybook": "start-storybook -p 6006 -c ./src/storybook --disable-telemetry",
        "build-storybook": "build-storybook -c ./src/storybook --disable-telemetry",
        "test-storybook": "test-storybook -c ./src/storybook",
        "test-storybook:ci": "concurrently -k -s first -n \"SB,TEST\" -c \"magenta,blue\" \"npm run build-storybook --quiet && npx http-server storybook-static --port 6006 --silent\" \"wait-on tcp:6006 && npm run test-storybook\"",
        "test-storybook:coverage": "test-storybook -c ./src/storybook --coverage",
        "test-storybook:coverage:ci": "concurrently -k -s first -n \"SB,TEST\" -c \"magenta,blue\" \"npm run build-storybook --quiet && npx http-server storybook-static --port 6006 --silent\" \"wait-on tcp:6006 && npm run test-storybook:coverage\"",
        "coverage-report": "cp coverage/storybook/coverage-storybook.json coverage/coverage-storybook.json && nyc report --reporter=lcov -t coverage --report-dir coverage"
    },
    "jest": {
        "preset": "jest-expo"
    },
    "dependencies": {
        "@react-native-async-storage/async-storage": "~1.17.3",
        "@react-navigation/drawer": "^6.5.0",
        "@react-navigation/native": "^6.0.2",
        "@react-navigation/native-stack": "^6.1.0",
        "date-fns": "^2.29.3",
        "expo": "~46.0.9",
        "expo-application": "^4.2.2",
        "expo-font": "~10.2.0",
        "expo-navigation-bar": "~1.3.0",
        "expo-splash-screen": "~0.16.2",
        "expo-status-bar": "~1.4.0",
        "formik": "^2.2.9",
        "i18next": "^21.9.1",
        "nanoid": "^4.0.0",
        "react": "18.0.0",
        "react-dom": "18.0.0",
        "react-i18next": "^11.18.6",
        "react-native": "0.69.6",
        "react-native-app-intro-slider": "^4.0.4",
        "react-native-flash-message": "^0.3.1",
        "react-native-gesture-handler": "~2.5.0",
        "react-native-modal": "^13.0.1",
        "react-native-reanimated": "~2.9.0",
        "react-native-safe-area-context": "4.3.1",
        "react-native-screens": "~3.15.0",
        "react-native-svg": "12.3.0",
        "react-native-web": "~0.18.7",
        "styled-components": "^5.3.5",
        "styled-system": "^5.1.5",
        "yup": "^0.32.11",
        "zustand": "^4.1.1",
        "expo-system-ui": "~1.3.0"
    },
    "devDependencies": {
        "@babel/core": "^7.12.9",
        "@storybook/addon-a11y": "^6.5.12",
        "@storybook/addon-coverage": "^0.0.5",
        "@storybook/addon-essentials": "^6.5.12",
        "@storybook/addon-interactions": "^6.5.12",
        "@storybook/addon-links": "^6.5.12",
        "@storybook/addon-react-native-web": "^0.0.18",
        "@storybook/addon-viewport": "^6.5.12",
        "@storybook/client-api": "^6.5.12",
        "@storybook/jest": "^0.0.10",
        "@storybook/react": "~6.5.12",
        "@storybook/react-native": "^5.3.25",
        "@storybook/react-native-server": "^5.3.23",
        "@storybook/test-runner": "^0.6.2",
        "@storybook/testing-library": "^0.0.13",
        "@types/react": "~18.0.14",
        "@types/react-native": "~0.69.1",
        "@types/styled-components-react-native": "^5.1.3",
        "@types/styled-system": "^5.1.15",
        "@typescript-eslint/eslint-plugin": "^5.38.0",
        "@typescript-eslint/parser": "^5.40.0",
        "babel-plugin-module-resolver": "^4.1.0",
        "concurrently": "^7.4.0",
        "eslint": "^8.25.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-config-airbnb-typescript": "^17.0.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-import-resolver-typescript": "^3.5.1",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-jsx-a11y": "^6.6.1",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-react": "^7.31.9",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-native": "^4.0.0",
        "eslint-plugin-react-native-a11y": "^3.2.1",
        "eslint-plugin-sort-keys-fix": "^1.1.2",
        "eslint-plugin-storybook": "^0.6.5",
        "eslint-plugin-typescript-sort-keys": "^2.1.0",
        "jest": "^26.6.3",
        "jest-expo": "~44.0.1",
        "prettier": "^2.7.1",
        "react-test-renderer": "18.0.0",
        "storybook-dark-mode": "^1.1.2",
        "storybook-react-i18next": "^1.1.2",
        "typescript": "~4.3.5"
    },
    "private": true
}
